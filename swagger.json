          "isPublic": {
            "type": "boolean"
          },
          "name": {
            "type": "string"
          }
        }
      },
      "AuthUser": {
        "type": "object",
        "required": ["email", "password"],
        "properties": {
          "_id": {
            "type": "string",
            "description": "id of user",
            "example": "5e9f8f8f8f8f8f8f8f8f8f8"
          },
          "email": {
            "type": "string",
            "description": "email of user",
            "example": "anymail@hotmail.com",
            "format": "email"
