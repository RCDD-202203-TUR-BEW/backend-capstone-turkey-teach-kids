          "username": {
            "type": "string",
            "description": "username of user",
            "example": "anyusername",
            "format": "username"
          },
          "password": {
            "type": "string",
            "description": "password of user",
            "example": "$2a$10$2M5ORDSG.lXG10tb9v1qduh9gHQoPW3dM5fmuIZefgHFPPVST638m",
            "format": "password"
          },
          "createdAt": {
            "type": "string",
            "description": "created at of user",
            "example": "2020-01-01T00:00:00.000Z"
